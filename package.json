{
  "name": "slot-manager-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:localhost": "cross-env NODE_ENV=localhost nest start --watch",
    "start:development": "cross-env NODE_ENV=development nest start --watch",
    "start:staging": "cross-env NODE_ENV=staging nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main.js",
    "pm2:localhost": "cross-env NODE_ENV=localhost npm run build && pm2 start dist/src/main.js",
    "pm2:staging": "cross-env NODE_ENV=staging npm run build && pm2 start dist/src/main.js",
    "pm2:production": "cross-env NODE_ENV=production npm run build && pm2 start dist/src/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "pre:commit": "npm run format && npm run lint",
    "pre:push": "npm run build",
    "typeorm": "npm run build && npx typeorm -d dist/data-source.js",
    "migration:generate": "cross-env NODE_ENV=localhost npm run typeorm -- migration:generate",
    "migration:up:localhost": "cross-env NODE_ENV=localhost npm run typeorm -- migration:run",
    "migration:down:localhost": "cross-env NODE_ENV=localhost npm run typeorm -- migration:revert",
    "migration:up:development": "cross-env NODE_ENV=development npm run typeorm -- migration:run",
    "migration:down:development": "cross-env NODE_ENV=development npm run typeorm -- migration:revert",
    "migration:up:staging": "cross-env NODE_ENV=staging npm run typeorm -- migration:run",
    "migration:down:staging": "cross-env NODE_ENV=staging npm run typeorm -- migration:revert",
    "migration:up:production": "cross-env NODE_ENV=production npm run typeorm -- migration:run",
    "migration:down:production": "cross-env NODE_ENV=production npm run typeorm -- migration:revert",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up",
    "docker:down": "docker-compose down",
    "docker:restart": "docker-compose restart"
  },
  "pre-commit": "pre:commit",
  "pre-push": "pre:push",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.705.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^4.1.2",
    "@nestjs/throttler": "^6.3.0",
    "@nestjs/typeorm": "^10.0.2",
    "@types/nodemailer": "^6.4.17",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "dotenv": "^16.4.7",
    "ejs": "^3.1.10",
    "exceljs": "^4.4.0",
    "helmet": "^8.0.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.9.16",
    "pg": "^8.13.1",
    "puppeteer": "^23.10.4",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/ejs": "^3.1.5",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
